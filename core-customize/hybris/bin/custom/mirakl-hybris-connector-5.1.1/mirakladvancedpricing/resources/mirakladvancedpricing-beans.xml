<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="beans.xsd">

    <bean class="com.mirakl.hybris.beans.OfferPricingCriteria">
        <description>A bean containing the offer pricing querying attributes</description>
        <property name="offerId" type="java.lang.String" />
        <property name="channelCode" type="java.lang.String" />
        <property name="date" type="java.util.Date" />
        <property name="customerOrganizationId" type="java.lang.String" />
        <property name="customerGroupId" type="java.lang.String" />
    </bean>

    <bean class="com.mirakl.hybris.beans.OfferPricingData">
        <property name="offerId" type="java.lang.String" />
        <property name="startDate" type="java.util.Date" />
        <property name="endDate" type="java.util.Date" />
        <property name="customerOrganizationId" type="java.lang.String" />
        <property name="customerGroupId" type="java.lang.String" />
    </bean>

